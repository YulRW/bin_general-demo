/**app.wxss**/
@import "colorui/main.wxss";
@import "colorui/icon.wxss";
@import '/myCss/iconfont.wxss';


.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 


page{
    /* 自定义相关配色 */
    --blue:#586b95;
    --grayLight:#f7f7f7;
    --gray:#7f7f7f;
    --grayActive:#e0e0e0;
    --blackLight:#2e2e2e;
    --blackLight2:#292929;
    --green:#07c160;
}

.active-gray:active{
    background-color: var(--grayActive);
}

.page{
    min-height: 100vh;
}
